[flake8]
ignore = E203, E266, E501, W503, F403, F401
exclude = 
    tests/*
    build/*
    amplify/backend/function/*/src/event.json
    path/to/typing_extensions.py
    ./my-layer/python/lib/python3.11/site-packages/*
     __pycache__/*
    *.pyc
max-line-length = 160
max-complexity = 18
select = B,C,E,F,W,T4,B9